{
  "manifest_version": 2,
  "name": "Convert Unix Timestamp",
  "version": "1.0.0.0",
  "icons" : {
    "16" : "./icons/clock_rock.png",
    "48" : "./icons/clock_rock.png",
    "128" : "./icons/clock_rock.png"
  },
  "browser_action": {
    "default_icon" : {
      "19" : "./icons/clock_rock.png",
      "38" : "./icons/clock_rock.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "clipboardWrite"
  ]
}